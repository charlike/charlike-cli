{
  "statements": 0,
  "functions": 0,
  "branches": 0,
  "lines": 0,
  "cache": true,
  "check-coverage": true,
  "require": "esm",
  "reporter": [
    "lcov",
    "text"
  ],
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "src/cjs-globals.js",
    "*.test.js",
    "**/*.test.js",
    "test"
  ]
}
